// ASMC Usage:
#define AoE_ASMC(Unit, AoE_ID, RotationID, CoordX, CoordY) "SVAL s1 Unit; SVAL s6 AoE_ID; SVAL s7 RotationID; SVAL sB CoordY<<16|CoordX; ASMC AoE_ASMC"
#define AoE_ASMC(Unit, AoE_ID, RotationID) "SVAL s1 Unit; SVAL s6 AoE_ID; SVAL s7 RotationID; ASMC AoE_ASMC"
/*
SVAL s1 (-1) // Unit
SVAL s6 0 // AoE ID
SVAL s7 0 // Rotation
SCOORD sB [10, 11] // Coords
ASMC AoE_ASMC
*/

ALIGN 4 
HealTileEffectEvent:
CHECK_COORDS 0x7FFF 
SADD sB sC s0 
AoE_ASMC(0x7FFF, 79, 0)
NoFade 
ENDA 

